
<form {{action "createModels" on="submit"}}>
  <button>
    Create
  </button>
</form>

<hr>

<ul>
  {{#each model as |one|}}
    <li>
      One: {{one.id}}
      {{one.twos}}
      <ul>
        {{#each one.twos as |two|}}
          <li>{{two}}</li>
        {{/each}}
      </ul>
    </li>
  {{/each}}
</ul>

{{outlet}}
